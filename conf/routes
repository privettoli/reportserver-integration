# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Pages
GET        /                                @org.spend.reportserver.integration.controller.WelcomeController.page()

# REST services
GET        /available_formats               @org.spend.reportserver.integration.controller.WelcomeController.supportedFormats
GET        /export/report:id.:format        @org.spend.reportserver.integration.controller.WelcomeController.export(id: Integer, format: String)

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                    controllers.Assets.at(path="/public", file)

    